#_,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,_#
  observeEvent(input$assignButton, {#
        userSamples$df[,"category"] <<- as.character(userSamples$df[,"category"])#
