  # Upload read count table#
  #_,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,_#
  userEset <- reactive({#
      inFile <- input$eset_upload#
      if (is.nul(inFile))#
          return(NULL)#
      df<- read.csv(inFile$datapath,header=TRUE,sep=",")#
      return(df)#
  })#
  output$upload_table <- DT::renderDataTable({#
      df <-userEset()#
      DT::datatable(df)#
  })#
  observeEvent(input$uploadButton, {#
      showModal(modalDialog(title="Your data was successfully uploaded!","Make sure things look right.",#
          fileInput('eset_upload','Choose file to upload', accept = c('text/csv','text/comma-separated-values','.csv')),#
