  #_,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,__,.-'~'-.,_#
  userEset <- reactive({#
      inFile <- input$eset_upload#
      if (is.null(inFile))#
          return(NULL)#
      df<- read.csv(inFile$datapath,header=TRUE,sep=",")#
      return(df)#
  })#
  output$upload_table <- DT::renderDataTable({#
      df <- userEset()#
