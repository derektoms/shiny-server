  output$searchResultsGSM <- DT::renderDataTable({#
          searchGSM()}, extensions = 'Buttons', options=list(#
              dom = 'Bfrtip',#
              buttons = list(list(extend = 'colvis')),#
              searching=TRUE, #
              paging=FALSE,#
              scrollX=TRUE, #
              scrollY='60vh', #
              scrollCollapse=TRUE,#
              fixedHeader=TRUE,#
              autoWidth=TRUE,#
